{
  "name": "plot-me",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "accounting": "^0.4.1",
    "ajv": "^6.2.1",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "plotly.js": "^1.35.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-plotly.js": "^1.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
